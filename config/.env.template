# CortexAi environment configuration
# Copy this file to .env and fill in your values
# DO NOT commit your actual .env file to version control!

# General settings
LOG_LEVEL=INFO
DEBUG=false

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database settings
VECTOR_DB_URL=your_vector_db_connection_string_here

# Provider settings
PROVIDER_DEFAULT=openai

# Web API settings
WEB_API_ENABLED=false
WEB_API_PORT=8000
WEB_API_HOST=127.0.0.1

# Nested configuration using double underscores
MEMORY__VECTOR__MAX_ITEMS=1000
TOOL__SCRAPER__TIMEOUT=30
